		  nlp2rdf_publications},
  owner		= {soeren},
  timestamp	= {2013.06.01},
  title		= {Integrating NLP using Linked Data},
  url		= {http://svn.aksw.org/papers/2013/ISWC_NIF/public.pdf},
  year		= {2013},
  bdsk-url-1	= {http://svn.aksw.org/papers/2013/ISWC_NIF/public.pdf}
}

